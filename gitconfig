[user]
	name = 
	email =

[alias]
  co = checkout
  cm = commit
  s = status
  b = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  undo = reset --soft HEAD^
  save = !git add . $1 && git cm -m save --no-verify $1
  stash-unapply = !git stash show -p | git apply -R

[rerere]
	enabled = false
